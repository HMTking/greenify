{
	"rewrites": [
		{ "source": "/((?!favicon|apple-touch-icon|site\\.webmanifest|browserconfig\\.xml).*)", "destination": "/index.html" }
	],
	"headers": [
		{
			"source": "/(favicon*|apple-touch-icon*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				},
				{
					"key": "Content-Type",
					"value": "image/svg+xml"
				}
			]
		},
		{
			"source": "/site.webmanifest",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/manifest+json"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/browserconfig.xml",
			"headers": [
				{
					"key": "Content-Type",
					"value": "application/xml"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/favicon.ico",
			"headers": [
				{
					"key": "Content-Type",
					"value": "image/x-icon"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	]
}
